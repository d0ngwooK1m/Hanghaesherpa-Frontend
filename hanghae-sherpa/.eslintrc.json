{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "ignorePatterns": ["node_modules/"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 7
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended", "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
